{
	"multipart": [
		{
			"apply": {
				"model": "promenade:block/sakura/fence/post"
			}
		},
		{
			"when": {
				"north": "true"
			},
			"apply": {
				"model": "promenade:block/sakura/fence/side",
				"uvlock": true
			}
		},
		{
			"when": {
				"east": "true"
			},
			"apply": {
				"model": "promenade:block/sakura/fence/side",
				"y": 90,
				"uvlock": true
			}
		},
		{
			"when": {
				"south": "true"
			},
			"apply": {
				"model": "promenade:block/sakura/fence/side",
				"y": 180,
				"uvlock": true
			}
		},
		{
			"when": {
				"west": "true"
			},
			"apply": {
				"model": "promenade:block/sakura/fence/side",
				"y": 270,
				"uvlock": true
			}
		}
	]
}
