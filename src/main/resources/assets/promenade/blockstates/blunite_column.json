{
	"multipart": [
		{
			"apply": {
				"model": "promenade:block/blunite/column/center"
			}
		},
		{
			"when": {
				"down": true
			},
			"apply": {
				"model": "promenade:block/blunite/column/end"
			}
		},
		{
			"when": {
				"up": true
			},
			"apply": {
				"model": "promenade:block/blunite/column/end",
				"x": 180,
				"uvlock": true
			}
		}
	]
}